{
  "name": "npm-compression",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "description": "A static site to demonstrate different compression algorithms for npm packages.",
  "scripts": {
    "build": "tsc -b && vite build",
    "dev": "vite",
    "lint": "biome check ./src",
    "format": "biome format --write ./src",
    "preview": "vite preview"
  },
  "dependencies": {
    "@bokuweb/zstd-wasm": "0.0.27",
    "brotli-wasm": "3.0.1",
    "chart.js": "4.4.9",
    "pako": "2.1.0",
    "preact": "10.26.9",
    "react-chartjs-2": "5.3.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@preact/preset-vite": "2.10.1",
    "@types/pako": "^2.0.3",
    "typescript": "5.8.3",
    "vite": "6.3.5"
  }
}